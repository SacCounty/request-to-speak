{
    "version": "0.2.0",
    "compounds": [
        {
            "name": "Dev Serve",
            "configurations": ["Launch Program","Launch Admin"]
        }
    ],
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceRoot}/index.js"
        },
        {
            "name": "Launch Wall",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000/wall.html",
            "webRoot": "${workspaceRoot}/client"
        },
        {
            "name": "Launch Board",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000/board.html",
            "webRoot": "${workspaceRoot}/client"
        },
        {
            "name": "Launch Kiosk",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000/kiosk.html",
            "webRoot": "${workspaceRoot}/client"
        },
        {
            "name": "Launch Admin",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:3000/#request",
            "webRoot": "${workspaceRoot}/client"
        }
    ]
}